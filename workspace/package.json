{
  "name": "workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:all": "concurrently -n w: npm:serve:single-spa:*",
    "build:single-spa:login": "ng build login --prod --deploy-url http://localhost:4200/",
    "serve:single-spa:login": "ng s --project login --disable-host-check --port 4200 --deploy-url http://localhost:4200/ --live-reload false",
    "build:single-spa:sidebar": "ng build sidebar --prod --deploy-url http://localhost:4201/",
    "serve:single-spa:sidebar": "ng s --project sidebar --disable-host-check --port 4201 --deploy-url http://localhost:4201/ --live-reload false",
    "build:single-spa:header": "ng build header --prod --deploy-url http://localhost:4202/",
    "serve:single-spa:header": "ng s --project header --disable-host-check --port 4202 --deploy-url http://localhost:4202/ --live-reload false",
    "build:single-spa:admin": "ng build admin --prod --deploy-url http://localhost:4203/",
    "serve:single-spa:admin": "ng s --project admin --disable-host-check --port 4203 --deploy-url http://localhost:4203/ --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~11.2.11",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/router": "~11.2.11",
    "rxjs": "~6.6.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^4.9.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3",
    "ng-zorro-antd": "^11.4.2",
    "@ant-design/icons-angular": "11.0.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.2-beta.0",
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular/cli": "~11.2.10",
    "@angular/compiler-cli": "~11.2.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
